{
  "extends": ["tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": ["config/**/*.js", "node_modules/**/*"]
  },
  "rules": {
    "adjacent-overload-signatures": true,
    "no-unsafe-any": true,
    "no-implicit-dependencies": true,
    "no-unused-variable": true,
    "no-duplicate-imports": true
  }
}
